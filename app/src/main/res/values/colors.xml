    <!--   a darker variant of the primary color, used for
           the status bar (on Android 5.0+) and contextual app bars -->
    <color name="colorPrimaryDark">#1C1C42</color>

    <!--   a secondary color for controls like checkboxes and text fields -->
    <color name="colorAccent">#58598F</color>

    <color name="colorTextPrimary">#E0E0E0</color>
    <color name="colorTextSecond">#BF3219</color>
    <color name="colorTextThird">#7171B5</color>
    <color name="colorBackgroundPrimary">#CACACA</color>
    <color name="colorBackgroundSecondary">#47488A</color>
    <color name="colorButtonStroke">#58598F</color>
    <color name="colorButtonRipple">#7171B5</color>
</resources>